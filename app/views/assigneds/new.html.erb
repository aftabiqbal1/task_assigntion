<h1> Assign Task </h1>

<%= form_with(model: @assigned, local: false) do |f| %>
  <div class="form-group">
    <%= f.label :user_id%>
    <%= f.text_field :user_id, class: "form-control", name: 'assigned[user_id]'%>
  </div>

  <div class="form-group">
    <%= f.label :task_id%>
    <%= f.text_field :task_id, class: "form-control",name: 'assigned[task_id]'%>
  </div>

  <div class="form-group">
    <%= f.submit "Assign Task"%>
  </div>
<%end%>

