<h1> Sign in </h1>

<%= form_with(url: signin_path) do |f| %>
<div class="form-group">
  <%= f.label :email %><br/>
  <%= f.email_field :email, class: "form-control" %>
</div>
 
<div class="form-group">
  <%= f.label :password %><br/>
  <%= f.password_field :password, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.submit "Sign In"%>
</div>
<% end %>